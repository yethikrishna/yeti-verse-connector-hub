# ðŸ§Š YETI AI PLATFORM - Environment Configuration
# ==============================================================================
# Copy this file to .env.local and fill in your API keys

# Frontend Configuration
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend API Keys (for Supabase Edge Functions)
# ==============================================================================

# OpenAI API - Primary AI provider
OPENAI_API_KEY=sk-your_openai_api_key_here

# OpenRouter API - Multi-model AI provider
OPENROUTER_API_KEY=sk-or-your_openrouter_api_key_here

# Google Gemini API - Google's AI models
GEMINI_API_KEY=your_gemini_api_key_here

# Novita API - Additional AI provider
NOVITA_API_KEY=your_novita_api_key_here

# A4F API - Image and Video Generation
A4F_API_KEY=your_a4f_api_key_here

# Optional: Additional AI Providers
# ==============================================================================
# Add more providers as needed
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# MISTRAL_API_KEY=your_mistral_api_key_here
# COHERE_API_KEY=your_cohere_api_key_here

# Database Configuration
# ==============================================================================
DATABASE_URL=your_database_connection_string
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OAuth and Integrations
# ==============================================================================
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Security
# ==============================================================================
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Development
# ==============================================================================
NODE_ENV=development
VITE_APP_ENV=development

# Development Configuration
VITE_HOST=0.0.0.0
VITE_PORT=8080

# Add other environment variables as needed
# VITE_SUPABASE_URL=your_supabase_url
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
